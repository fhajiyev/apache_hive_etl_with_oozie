
USE SVC_CSW1;

INSERT OVERWRITE TABLE DMP_POC_BIZ_COMPANY
SELECT
 CASE WHEN REGEXP_REPLACE(MAIN_CATEGORY_CD, ',', ' ') = 'null'                     THEN '' ELSE REGEXP_REPLACE(MAIN_CATEGORY_CD, ',', ' ') END,
 CASE WHEN REGEXP_REPLACE(SUB_CATEGORY_CD, ',', ' ') = 'null'                      THEN '' ELSE REGEXP_REPLACE(SUB_CATEGORY_CD, ',', ' ') END,
 CASE WHEN REGEXP_REPLACE(BIZ_COMPANY_SQ, ',', ' ') = 'null'                       THEN '' ELSE REGEXP_REPLACE(BIZ_COMPANY_SQ, ',', ' ') END

FROM TEMP_DMP_POC_BIZ_COMPANY
;
