
USE SVC_CSW1;


INSERT OVERWRITE TABLE DMP_POC_BIZ_SITE
SELECT
 CASE WHEN REGEXP_REPLACE(SITE_CD, ',', ' ') = 'null'                     THEN '' ELSE REGEXP_REPLACE(SITE_CD, ',', ' ') END,
 CASE WHEN REGEXP_REPLACE(ADV_BRAND_NM, ',', ' ') = 'null'                THEN '' ELSE REGEXP_REPLACE(ADV_BRAND_NM, ',', ' ') END

FROM TEMP_DMP_POC_BIZ_SITE
;
