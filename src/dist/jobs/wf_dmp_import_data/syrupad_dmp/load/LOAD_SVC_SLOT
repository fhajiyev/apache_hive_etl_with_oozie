
USE SVC_CSW1;


INSERT OVERWRITE TABLE DMP_SVC_SLOT
SELECT 
 CASE WHEN REGEXP_REPLACE(SLOT_SQ, ',', ' ') = 'null' THEN '' ELSE REGEXP_REPLACE(SLOT_SQ, ',', ' ') END,
 CASE WHEN REGEXP_REPLACE(SLOT_NM, ',', ' ') = 'null' THEN '' ELSE REGEXP_REPLACE(SLOT_NM, ',', ' ') END

FROM TEMP_DMP_SVC_SLOT
WHERE 
cast(SLOT_SQ as bigint) IS NOT NULL
;
