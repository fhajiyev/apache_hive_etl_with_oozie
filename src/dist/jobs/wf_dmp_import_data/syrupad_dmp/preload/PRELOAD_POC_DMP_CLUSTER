


use SVC_CSW1;

set hivevar:MetaPathHive;

DROP TABLE IF EXISTS TEMP_DMP_POC_DMP_CLUSTER;

CREATE TABLE TEMP_DMP_POC_DMP_CLUSTER
(

   CLUSTER_SQ       STRING,
   STATUS_CD        STRING,
   CLUSTER_NM       STRING,
   CLUSTER_TYPE_CD  STRING,
   MAIN_CATEGORY_SQ STRING,
   SUB_CATEGORY_SQ  STRING,
   KRUX_SEG_NM      STRING,
   KRUX_SEG_ID      STRING,
   KRUX_SEG_ID_LONG STRING,
   MEMO             STRING,
   DMP_FL           STRING,
   CREATE_DT        STRING

)
ROW FORMAT DELIMITED FIELDS TERMINATED BY '|';


LOAD DATA INPATH '${hivevar:MetaPathHive}/poc_dmp_cluster/part-m-00000' INTO TABLE TEMP_DMP_POC_DMP_CLUSTER;
