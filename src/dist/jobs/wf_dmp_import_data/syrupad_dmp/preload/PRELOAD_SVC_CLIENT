



use SVC_CSW1;

set hivevar:MetaPathHive;

DROP TABLE IF EXISTS TEMP_DMP_SVC_CLIENT;

CREATE TABLE TEMP_DMP_SVC_CLIENT
(
   
    CLIENT_SQ          STRING,
    MEDIA_SQ           STRING,
    SLOT_SQ            STRING,
    FILL_RATE_SQ       STRING,
    PAGE_ID            STRING,
    POSITION_ID        STRING,
    CLIENT_NM          STRING,
    LOWER_CLIENT_NM    STRING,
    CLIENT_ID          STRING,
    CHANNEL_CD         STRING,
    CLIENT_URL         STRING,
    MWEB_CHARSET_CD    STRING,
    OS_CD              STRING,
    CLIENT_STATUS_CD   STRING,
    CLIENT_CATEGORY_CD STRING,
    BILL_TYPE_BIT      STRING,
    RS_RATE            STRING,
    CALC_TYPE          STRING,
    CREATIVE_TYPE_BIT  STRING,
    REJECT_REASON      STRING,
    AVG_REQUEST_CNT    STRING,
    FILLRATE_UPDATE_DT STRING,
    CREATE_DT          STRING,
    UPDATE_DT          STRING,
    CREATE_ACCOUNT_SQ  STRING,
    UPDATE_ACCOUNT_SQ  STRING

)
ROW FORMAT DELIMITED FIELDS TERMINATED BY '|';


LOAD DATA INPATH '${hivevar:MetaPathHive}/svc_client/part-m-00000' INTO TABLE TEMP_DMP_SVC_CLIENT;
