

use SVC_CSW1;

set hivevar:MetaPathHive;

DROP TABLE IF EXISTS TEMP_DMP_SVC_MEDIA;

CREATE TABLE TEMP_DMP_SVC_MEDIA
(
   
   MEDIA_SQ             STRING,
   ACCOUNT_SQ           STRING,
   MEDIA_NM             STRING,
   MEDIA_ID             STRING,
   SSP_DELIVERY_API_URL STRING,
   SSP_STAT_API_URL     STRING,
   REMARK               STRING,
   MEDIA_TYPE_CD        STRING,
   MEDIA_STATUS_CD      STRING,
   MULTI_SLOT_FL        STRING,
   MEDIATION_FL         STRING,
   MCPSUB_RS_RATE       STRING,
   CREATE_DT            STRING,
   UPDATE_DT            STRING,
   CREATE_ACCOUNT_SQ    STRING,
   UPDATE_ACCOUNT_SQ    STRING

)
ROW FORMAT DELIMITED FIELDS TERMINATED BY '|';



LOAD DATA INPATH '${hivevar:MetaPathHive}/svc_media/part-m-00000' INTO TABLE TEMP_DMP_SVC_MEDIA;
