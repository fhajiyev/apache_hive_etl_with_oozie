
use SVC_CSW1;

set hivevar:MetaPathHive;

DROP TABLE IF EXISTS TEMP_DMP_SVC_SLOT;

CREATE TABLE TEMP_DMP_SVC_SLOT
(

   SLOT_SQ           STRING,
   SLOT_NM           STRING,
   SLOT_WIDTH        STRING,
   SLOT_HEIGHT       STRING,
   DELIVERY_MODE_CD  STRING,
   SELF_FL           STRING,
   AGENCY_FL         STRING,
   JPG_MAX_SIZE_AMT  STRING,
   PNG_MAX_SIZE_AMT  STRING,
   GIF_MAX_SIZE_AMT  STRING,
   STATUS_GB         STRING,
   CREATE_DT         STRING,
   UPDATE_DT         STRING,
   CREATE_ACCOUNT_ID STRING,
   UPDATE_ACCOUNT_ID STRING

)
ROW FORMAT DELIMITED FIELDS TERMINATED BY '|';

LOAD DATA INPATH '${hivevar:MetaPathHive}/svc_slot/part-m-00000' INTO TABLE TEMP_DMP_SVC_SLOT;
